<% provide(:title, "Sign in") %>
<div class ="row">
	<div class ="span4 offset1">
		<h3>Sign in</h3>
		<%= form_for(:session, url: sessions_path) do |f| %>
		<%= f.label :email %>
		<%= f.text_field :email %>

		<%= f.label :password %>
		<%= f.password_field :password %>

		<%= f.submit 'Sign in', class: "btn btn-primary" %>
		<% end %>
	</div>
</div>